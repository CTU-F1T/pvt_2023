<launch>

	<arg name="world" doc="Name of a world within 'stage_worlds' (storage/stage/world)"/>

	<node
		pkg="stage_ros"
		type="stageros"
		name="stage_simulator"
		output="screen"
		args="$(find storage)/stage/world/$(arg world).world"
	/>

</launch>
